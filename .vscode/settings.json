    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#053b25",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#8b580c",
    "statusBar.background": "#5c3a08",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#8b580c",
    "statusBarItem.remoteBackground": "#5c3a08",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#5c3a08",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#5c3a0899",

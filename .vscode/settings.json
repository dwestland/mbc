{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.format": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#1f98ad",
    "activityBar.activeBorder": "#f5c9ef",
    "activityBar.background": "#1f98ad",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#f5c9ef",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#1f98ad",
    "statusBar.background": "#177282",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1f98ad",
    "statusBarItem.remoteBackground": "#177282",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#177282",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#17728299",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#177282"
}
